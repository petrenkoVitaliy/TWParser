{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2017",
        "baseUrl": "./",
        "outDir": "./public",
        "declaration": true,
        "removeComments": true,
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "incremental": true,
        "skipLibCheck": true,
        "strictBindCallApply": false,
        "forceConsistentCasingInFileNames": false,
        "noFallthroughCasesInSwitch": false,
        "noImplicitAny": true,
        "strictNullChecks": true
    }
}
